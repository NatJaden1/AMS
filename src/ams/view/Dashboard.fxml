<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox fx:controller="ams.controller.DashboardController"
      spacing="15" alignment="CENTER"
      xmlns:fx="http://javafx.com/fxml">
    <padding>
        <Insets top="30" right="30" bottom="30" left="30"/>
    </padding>

    <Button text="Manage Students" onAction="#openStudentManager" prefWidth="200"/>
    <Button text="Manage Teachers" prefWidth="200"/>
    <Button text="Manage Classes" prefWidth="200"/>
    <Button text="Manage Subjects" prefWidth="200"/>
    <Button text="Manage Exams" prefWidth="200"/>
</VBox>
